password = input("Enter password: ")
score = 0
has_upper = False
has_lower = False
has_digit = False
has_special = False


if len(password) >= 8:
    score += 1

for char in password:
    if char.isupper():
        has_upper = True
    elif char.islower():
        has_lower = True
    elif char.isdigit():
        has_digit = True
    else:
        has_special = True

if has_upper == True:
    score += 1
if has_lower == True:
    score += 1
if has_digit == True:
    score += 1
if has_special == True:
    score += 1

if score <= 2:
    print("Password Strength: Weak")
elif score <= 4:
    print("Password Strength: Medium")
else:
    print("Password Strength: Strong")
